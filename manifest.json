{
  "manifest_version": 2,

  "name": "My First Extension",
  "description": "Me playing around with extensions",
  "version": "1.0",


  "permissions": [
    "tabs",
    "https://www.youtube.com/*"
  ],

  "omnibox": {"keyword" : "h"},

  "background": {
     "scripts": ["background.js"],
     "persistent": false
    },

   "content_scripts": [{
      "js": ["jquery-3.1.1.slim.min.js", "search.js"],

      "matches" : [ "https://www.youtube.com/*" ]
    }]

}
