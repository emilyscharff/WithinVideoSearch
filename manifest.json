{
  "manifest_version": 2,

  "name": "My First Extension",
  "description": "Me playing around with extensions",
  "version": "1.0",


  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "omnibox": {
    "keyword" : "omnix",
   "matches": [
        "https://youtube.com/*"
      ]},

  "background": {
     "scripts": ["background.js"]
    },

   "content_scripts": {
      "js": ["search.js"],

      "matches": ["https://youtube.com/*"]
    }

}
