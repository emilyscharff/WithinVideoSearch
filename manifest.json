{
  "manifest_version": 2,

  "name": "My First Extension",
  "description": "Me playing around with extensions",
  "version": "1.0",


  "permissions": [
    "tabs",
    "*://*/*"
  ],

  "omnibox": {"keyword" : "h"},

  "background": {
     "scripts": ["background.js"],
     "persistent": false
    },

   "content_scripts": [{
      "js": ["search.js"],

      "matches" : [ "*://*/*" ]
    }]

}
